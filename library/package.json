{
  "name": "vue-onboarding-tour",
  "version": "0.0.9-beta.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-onboarding-tour.es.js",
      "require": "./dist/vue-onboarding-tour.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "node convert-tailwind-to-css.js && vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && node convert-tailwind-to-css.js && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "@vueuse/integrations": "^11.2.0",
    "dompurify": "^3.2.3",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@storybook/vue3": "^8.4.2",
    "@storybook/vue3-vite": "^8.4.2",
    "@tailwindcss/postcss": "^4.0.8",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.14.0",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-vue": "^9.30.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ]
}
